<div class="row o-highlights o-highlights-five" id="highlights-five">
  <% for(let jogo of jogos) { %>
  <div class="o-highlights__content o-highlights-five__content o-col-and-5"
    style="padding: 0; display: flex; box-shadow: 0 0 15px 0 hsla(0,0%,73%,.3);"
    data-aos="fade-up" data-aos-duration="1000">
    <div class="o-highlights_content-animate animate-grow-shadow c-category-<%= jogo.tema_id %>">
      <% if(flagClicavel) { %>
        <a href="/jogo?id=<%= jogo.id %>">
          <% } %>
          <div class="o-highlights__header o-highlights-five__header" style="background: url('/buscaImagem/<%= jogo.foto %>') center center no-repeat;">
            <h3 class="c-category-<%= jogo.tema_id %>"><%= (jogo.notaJogo / 2).toFixed(1) %></h3>
          </div>
          <div class="o-highlights__description o-highlights-five__description">
            <h2><%= jogo.nome %></h2>
            <h6><%= (jogo.notaJogo / 2).toFixed(1) %></h6>
            <a href='#'class='c-jogo__tag'><%= jogo.tema.nome %></a>
            <h6><%= jogo.totalFavorito %></h6>
            <h6><%= jogo.totalJoguei %></h6>

          </div>
        </a>
    </div>
    
  </div>
  <% } %>
</div>
