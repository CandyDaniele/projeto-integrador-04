<div class="row o-highlights o-highlights-three u-highlights-grey" id="highlights-five">
  <% for(let jogo of jogos) { %>
  <div class="o-highlights__content o-highlights-three__content o-col-and-3 animate-float-shadow" data-aos="fade-up"
    data-aos-duration="1000"
    style="padding: 0; display: flex; flex-direction: column; width: 200px; box-shadow: 0 0 35px 0 hsla(0,0%,73%,.8);">
    <a href="/jogo?id=<%= jogo.id %>">
      <div class="o-highlights__header o-highlights-three__header"
        style="border-radius: 50px 50px 0 0; box-sizing: content-box; overflow: hidden; height: 200px;">
        <img src="/buscaImagem/<%= jogo.foto %>" alt="" style="height: auto;">
      </div>
      <div class="o-highlights__description o-highlights-three__description"
        style="padding-bottom: 10px; padding-left: 10px; min-height: 60px; max-height: 60px;">
        <h2><%= jogo.nome %></h2>
      </div>
  </div>
  </a>
  <% } %>
  <% if(jogos.length == 0) { %>
    <p>Nenhum jogo adicionado.</p>
  <% } %>
</div>

<script src="js/scriptListaJogos.js" defer></script>